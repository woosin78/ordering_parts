		<InsertTable visible="true" title="표 삽입"/>
		<DeleteTable visible="true" title="표 삭제"/>
		<split/>
		<InsertRowColumn visible="true" title="행/열 삽입">
			<InsertRowAbove visible="true" title="위에 행 삽입"/>
			<InsertRowBelow visible="true" title="아래에 행 삽입"/>
			<InsertColumnLeft visible="true" title="왼쪽에 열 삽입"/>
			<InsertColumnRight visible="true" title="오른쪽에 열 삽입"/>
		</InsertRowColumn>
		<DeleteRowColumn visible="true" title="행/열 삭제">
			<DeleteRow visible="true" title="행 삭제"/>
			<DeleteColumn visible="true" title="열 삭제"/>
		</DeleteRowColumn>
		<split/>
		<MergeCell visible="true" title="셀 병합"/>
		<SplitCell visible="true" title="셀 분할"/>
