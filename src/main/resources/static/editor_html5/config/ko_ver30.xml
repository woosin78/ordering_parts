			<VerticalBase visible="true" title="기준선"/>
			<AlignTop visible="true" title="위쪽"/>
			<AlignMiddle visible="true" title="가운데"/>
			<AlignBottom visible="true" title="아래쪽"/>
		</VerticalAlign>
		<split/>
		<TableProperty visible="true" title="표 속성"/>
		<CellProperty visible="true" title="셀 속성"/>
	</Table>
	
	<!-- 서식 메뉴 설정. -->
	<Style visible="true" title="서식"> 
		<Bold visible="true" title="굵게"/>
		<Italic visible="true" title="기울임꼴"/>
		<Underline visible="true" title="밑줄"/>
		<Strikethrough visible="true" title="취소선"/>
		<Superscript visible="true" title="위 첨자"/>
		<Subscript visible="true" title="아래 첨자"/>
		<split/>
		<RemoveFormatting visible="true" title="서식 제거"/>
		<split/>
		<Bullets visible="true" title="글머리 기호"/>
		<Numbering visible="true" title="글머리 번호"/>
		<split/>
		<IncreaseIndent visible="true" title="들여쓰기"/>
		<DecreaseIndent visible="true" title="내어쓰기"/>
		<split/>
		<Align visible="true" title="정렬">
			<AlignLeft visible="true" title="왼쪽 정렬"/>
			<AlignCenter visible="true" title="가운데 정렬"/>
			<AlignRight visible="true" title="오른쪽 정렬"/>
			<Justify visible="true" title="양쪽 정렬"/>
		</Align>
	</Style>
	
	<!-- 도구 메뉴 설정. -->
	<Tool visible="true" title="도구"> 
		<PageProperty visible="true" title="문서 정보"/>
	</Tool>
	
	<!-- 도움말 메뉴 설정. -->
	<Help visible="true" title="도움말">
		<UserGuide visible="true" title="사용자 가이드"/>
		<About visible="true" title="제품 정보"/>
	</Help>	
</menubar>

