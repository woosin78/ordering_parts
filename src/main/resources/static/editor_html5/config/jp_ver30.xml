		<split/>
		<IncreaseIndent visible="true" title="インデント"/>
		<DecreaseIndent visible="true" title="アウトデント"/>
		<split/>
		<Align visible="true" title="整列">
			<AlignLeft visible="true" title="左整列"/>
			<AlignCenter visible="true" title="中央に配置"/>
			<AlignRight visible="true" title="右整列"/>
			<Justify visible="true" title="両方整列"/>
		</Align>
	</Style>
	
	<!-- 도구 메뉴 설정. -->
	<Tool visible="true" title="道具"> 
		<PageProperty visible="true" title="文書情報"/>
	</Tool>
	
	<!-- 도움말 메뉴 설정. -->
	<Help visible="true" title="アドバイス">
		<UserGuide visible="true" title="ユーザーガイド"/>
		<About visible="true" title="製品情報"/>
	</Help>	
