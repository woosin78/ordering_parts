		<Italic visible="true" title="実施することになった"/>
		<Underline visible="true" title="下線"/>
		<Strikethrough visible="true" title="取り消し線"/>
		<Superscript visible="true" title="上付き字"/>
		<Subscript visible="true" title="下付き字"/>
