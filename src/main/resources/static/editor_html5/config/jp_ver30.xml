			<DeleteRow visible="true" title="行の削除"/>
			<DeleteColumn visible="true" title="熱削除"/>
		</DeleteRowColumn>
		<split/>
		<MergeCell visible="true" title="セル併合"/>
		<SplitCell visible="true" title="セル分割"/>
		<split/>
		<SelectCell visible="true" title="セル選択"/>
		<ResizeCell visible="true" title="セルのサイズ調節">
			<ResizeWidth visible="true" title="幅を同じに"/>
			<ResizeHeight visible="true" title="高さを同様に"/>
		</ResizeCell>
		<VerticalAlign visible="true" title="垂直方向の並び替え">
			<VerticalBase visible="true" title="ベースライン"/>
			<AlignTop visible="true" title="上側"/>
			<AlignMiddle visible="true" title="うちに"/>
			<AlignBottom visible="true" title="下つ方"/>
		</VerticalAlign>
		<split/>
		<TableProperty visible="true" title="表のプロパティ"/>
		<CellProperty visible="true" title="セルプロパティ"/>
	</Table>
	
