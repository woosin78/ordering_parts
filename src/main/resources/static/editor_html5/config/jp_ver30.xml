		<InsertTable visible="true" title="表挿入"/>
		<DeleteTable visible="true" title="表削除"/>
		<split/>
		<InsertRowColumn visible="true" title="行/列の挿入">
			<InsertRowAbove visible="true" title="上に行挿入"/>
			<InsertRowBelow visible="true" title="下に行挿入"/>
			<InsertColumnLeft visible="true" title="左に列の挿入"/>
			<InsertColumnRight visible="true" title="右に列の挿入"/>
		</InsertRowColumn>
