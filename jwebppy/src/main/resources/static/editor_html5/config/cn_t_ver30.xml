	</File>
	
	<!-- 편집 메뉴 설정. -->
	<Edit visible="true" title="編輯"> 
		<Undo visible="true" title="回返"/>
		<Redo visible="true" title="重新實行"/>
		<split/>
		<Cut visible="true" title="剪掉"/>
		<Copy visible="true" title="輻射"/>
		<Paste visible="true" title="粘貼"/>
		<split/>
		<SelectAll visible="true" title="全部選擇"/>
		<split/>		
		<Replace visible="true" title="換貨/替換"/>
	</Edit>
		
	<!-- 삽입 메뉴 설정. -->
	<Insert visible="true" title="插入"> 
		<Background visible="true" title="文件背景圖"/>
		<InsertImage visible="true" title="形象"/>
		<Object visible="true" title="客體">
			<InsertMediaLink visible="true" title="外部視頻共享"/>
