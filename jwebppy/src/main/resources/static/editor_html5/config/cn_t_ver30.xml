<?xml version="1.0"?>

<xfeEditor>


<menubar visible="true">
	
	<!-- 파일 메뉴 설정. -->
	<File visible="true" title="文件">
		<NewFile visible="true" title="新條目"/>
		<OpenFile visible="true" title="文件熱"/>
		<split/>		
		<Layout visible="true" title="版面佈局"/>
		<split/>
		<SaveFile visible="true" title="貯藏"/>
		<AutoSave visible="true" title="自動儲存"/>
