		<Html visible="true" title="HTML"/>
		<Preview visible="true" title="預覽"/>
		<Text visible="true" title="TEXT"/>		
	</View>
	
	<!-- 표 메뉴 설정. -->
	<Table visible="true" title="表格">		
		<InsertTable visible="true" title="插入表"/>
		<DeleteTable visible="true" title="刪除表"/>
		<split/>
		<InsertRowColumn visible="true" title="行/熱入列">
			<InsertRowAbove visible="true" title="往上插行"/>
			<InsertRowBelow visible="true" title="往下插進去"/>
			<InsertColumnLeft visible="true" title="向左轉"/>
			<InsertColumnRight visible="true" title="右邊放入右邊"/>
		</InsertRowColumn>
		<DeleteRowColumn visible="true" title="刪除行/熱">
			<DeleteRow visible="true" title="刪除行"/>
			<DeleteColumn visible="true" title="熱消熱"/>
		</DeleteRowColumn>
		<split/>
		<MergeCell visible="true" title="合併單元格"/>
		<SplitCell visible="true" title="拆分單元格"/>
		<split/>
		<SelectCell visible="true" title="單元選擇"/>
		<ResizeCell visible="true" title="調整單元格大小">
			<ResizeWidth visible="true" title="固定寬度"/>
			<ResizeHeight visible="true" title="固定高度"/>
		</ResizeCell>
		<VerticalAlign visible="true" title="垂直排序">
			<VerticalBase visible="true" title="基準線"/>
			<AlignTop visible="true" title="上面"/>
			<AlignMiddle visible="true" title="中間"/>
			<AlignBottom visible="true" title="下邊"/>
		</VerticalAlign>
		<split/>
		<TableProperty visible="true" title="表屬性"/>
		<CellProperty visible="true" title="單元格屬性"/>
	</Table>
	
	<!-- 서식 메뉴 설정. -->
	<Style visible="true" title="棲息"> 
		<Bold visible="true" title="粗粗地"/>
		<Italic visible="true" title="斜轉角"/>
		<Underline visible="true" title="下劃線"/>
		<Strikethrough visible="true" title="駁船線"/>
		<Superscript visible="true" title="上加字"/>
		<Subscript visible="true" title="上下尖尺"/>
		<split/>
		<RemoveFormatting visible="true" title="擦除格式"/>
		<split/>
		<Bullets visible="true" title="記號記號"/>
		<Numbering visible="true" title="筆記本兒"/>
		<split/>
		<IncreaseIndent visible="true" title="寫下"/>
		<DecreaseIndent visible="true" title="寫書"/>
		<split/>
		<Align visible="true" title="排序">
			<AlignLeft visible="true" title="左邊排序"/>
			<AlignCenter visible="true" title="中間排序"/>
			<AlignRight visible="true" title="右列排序"/>
			<Justify visible="true" title="兩邊排序"/>
		</Align>
	</Style>
	
	<!-- 도구 메뉴 설정. -->
	<Tool visible="true" title="工具"> 
		<PageProperty visible="true" title="環境設置"/>
	</Tool>
	
	<!-- 도움말 메뉴 설정. -->
	<Help visible="true" title="幫助">
		<UserGuide visible="true" title="用戶指南"/>
		<About visible="true" title="產品信息"/>
	</Help>	
