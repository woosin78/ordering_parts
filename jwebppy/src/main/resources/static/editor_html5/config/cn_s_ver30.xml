		<Print visible="true" title="印刷"/>
		<Pagebreak visible="true" title="页面分割"/>
	</File>
	
	<!-- 편집 메뉴 설정. -->
	<Edit visible="true" title="编辑"> 
		<Undo visible="true" title="回返"/>
		<Redo visible="true" title="重新实行"/>
		<split/>
		<Cut visible="true" title="剪掉"/>
		<Copy visible="true" title="辐射"/>
		<Paste visible="true" title="粘贴"/>
		<split/>
		<SelectAll visible="true" title="全部选择"/>
		<split/>		
		<Replace visible="true" title="换货/替换"/>
	</Edit>
		
