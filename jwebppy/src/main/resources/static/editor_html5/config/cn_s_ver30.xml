	<Insert visible="true" title="插入"> 
		<Background visible="true" title="文件背景图"/>
		<InsertImage visible="true" title="形象"/>
		<Object visible="true" title="客体">
			<InsertMediaLink visible="true" title="外部视频共享"/>
