	</View>
	
	<!-- 표 메뉴 설정. -->
	<Table visible="true" title="表格">		
		<InsertTable visible="true" title="插入表"/>
		<DeleteTable visible="true" title="删除表"/>
		<split/>
		<InsertRowColumn visible="true" title="行/热入列">
			<InsertRowAbove visible="true" title="往上插行"/>
			<InsertRowBelow visible="true" title="往下插进去"/>
			<InsertColumnLeft visible="true" title="向左转"/>
			<InsertColumnRight visible="true" title="右边放入右边"/>
		</InsertRowColumn>
		<DeleteRowColumn visible="true" title="删除行/热">
			<DeleteRow visible="true" title="删除行"/>
			<DeleteColumn visible="true" title="热消热"/>
		</DeleteRowColumn>
		<split/>
		<MergeCell visible="true" title="合并单元格"/>
		<SplitCell visible="true" title="拆分单元格"/>
		<split/>
		<SelectCell visible="true" title="单元选择"/>
		<ResizeCell visible="true" title="调整单元格大小">
			<ResizeWidth visible="true" title="固定宽度"/>
			<ResizeHeight visible="true" title="固定高度"/>
		</ResizeCell>
		<VerticalAlign visible="true" title="垂直排序">
			<VerticalBase visible="true" title="基准线"/>
			<AlignTop visible="true" title="上面"/>
			<AlignMiddle visible="true" title="中间"/>
			<AlignBottom visible="true" title="下边"/>
		</VerticalAlign>
		<split/>
		<TableProperty visible="true" title="表属性"/>
		<CellProperty visible="true" title="单元格属性"/>
	</Table>
	
	<!-- 서식 메뉴 설정. -->
	<Style visible="true" title="栖息"> 
		<Bold visible="true" title="粗粗地"/>
		<Italic visible="true" title="斜转角"/>
		<Underline visible="true" title="下划线"/>
		<Strikethrough visible="true" title="驳船线"/>
		<Superscript visible="true" title="上加字"/>
		<Subscript visible="true" title="上下尖尺"/>
		<split/>
		<RemoveFormatting visible="true" title="擦除格式"/>
		<split/>
		<Bullets visible="true" title="记号记号"/>
		<Numbering visible="true" title="笔记本儿"/>
		<split/>
		<IncreaseIndent visible="true" title="写下"/>
		<DecreaseIndent visible="true" title="写书"/>
		<split/>
		<Align visible="true" title="排序">
			<AlignLeft visible="true" title="左边排序"/>
			<AlignCenter visible="true" title="中间排序"/>
			<AlignRight visible="true" title="右列排序"/>
			<Justify visible="true" title="两边排序"/>
		</Align>
	</Style>
	
	<!-- 도구 메뉴 설정. -->
	<Tool visible="true" title="工具"> 
		<PageProperty visible="true" title="环境设置"/>
	</Tool>
	
	<!-- 도움말 메뉴 설정. -->
	<Help visible="true" title="帮助">
		<UserGuide visible="true" title="用户指南"/>
		<About visible="true" title="产品信息"/>
	</Help>	
