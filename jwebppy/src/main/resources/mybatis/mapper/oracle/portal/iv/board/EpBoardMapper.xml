<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.jwebppy.portal.iv.board.mapper.EpBoardMapper">

	<select id="findBoard" parameterType="EpBoardDto" resultType="EpBoardEntity">
		select
		a.*
		from
		ep_board a
		where
		a.fg_delete='N'
		and a.b_seq=#{bSeq}
	</select>

</mapper>