<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.jwebppy.platform.mgmt.board.mapper.BoardMapper">

	<select id="findBoard" parameterType="Integer" resultType="BoardEntity">
		select
		a.*
		from
		pltf_board a
		where
		a.b_seq=#{bSeq}		
	</select>

</mapper>